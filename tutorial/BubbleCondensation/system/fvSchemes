FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default         Euler;
}

interpolationSchemes
{
    default         linear;
}

divSchemes
{
	div(rhoPhi,U)                 Gauss vanLeerV; //Gauss linearUpwindV grad(U);;
	div(phi,alpha)                Gauss vanLeer; 
        //div(phirb,alpha)              bounded Gauss interfaceCompression; 
        div(phirb,alpha)              bounded Gauss limitedLinear 1; //changed by MMG
	div(R)                        Gauss linear;
	//div(rhoPhi,U)                 Gauss linearUpwind grad(U);
        //added by MMG
        //div(((rho*nuEff)*dev2(T(grad(U))))) Gauss linear;
	div(rhoPhi,U)                 Gauss vanLeer;
	div((muEff*dev(T(grad(U)))))  Gauss linear;
	default                       Gauss linear;
}

gradSchemes
{
    default         Gauss linear;
}

laplacianSchemes
{
    default         Gauss linear limited 0.5;
}

snGradSchemes
{
    default              limited 0.5;
}

fluxRequired
{
    default         none;
    p_rgh;
    pcorr;
    alpha1;
}


