FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    BotWall
    {
        type            fixedFluxPressure;
        gradient        uniform 0;
        value           uniform 0;
    }    

    FreeStream
    {
        type            totalPressure;
        rho             rho;
        psi             none;
        gamma           1;
        p0              uniform 0;
        value           uniform 0;
    }

    FrontNBack
    {
        type            empty;
    }

	LeftSide
	{
		type            symmetryPlane;
	}

	RightSide
	{
		type            symmetryPlane;
	}

}


